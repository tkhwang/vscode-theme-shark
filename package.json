{
    "name": "theme-shark",
    "displayName": "Shark Theme",
    "description": "Shark a VSCode theme",
    "version": "0.10.1",
    "publisher": "willyelm",
    "repository": "https://github.com/willyelm/vscode-theme-shark.git",
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "settings": [
            {
                "id": "shark",
                "label": "Shark",
                "path": "./settings/settings.json"
            }
        ],
        "themes": [
            {
                "label": "Dark (Shark)",
                "uiTheme": "vs-dark",
                "path": "./themes/dark-theme.json"
            },
            {
              "label": "Light (Shark)",
              "uiTheme": "vs-dark",
              "path": "./themes/light-theme.json"
          }
        ],
        "iconThemes": [
            {
                "id": "shark",
                "label": "Shark",
                "path": "./fileicons/fileicon-theme.json"
            }
        ]
    }
}